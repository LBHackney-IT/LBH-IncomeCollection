uuid = '<%= @preview.dig(:uuid) %>';
errors = '<%= @preview.dig(:errors).to_json %>';
errors = JSON.parse(errors.replace(/&quot;/g, '"'));
pay_ref = '<%= @preview.dig(:case, :payment_ref) %>'

if (errors.length === 0) {
  $('#successful_table').append("<%= j (render partial: 'successful_table', preview: @preview) %>");
  increment_success_counter();

} else {

  $('#errors_table').append("<%= j (render partial: 'errors_table', preview: @preview) %>");
  increment_fail_counter();
}
